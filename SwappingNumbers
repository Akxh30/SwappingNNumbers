#include <stdio.h>
  int main()
{
    int x, y;
    
    printf("Enter the Value of x ");
    scanf("%d", &x);
   
   printf("\nEnter the Value of y ");
    scanf("%d", &y);
  
    int temp = x;
   
   x = y;
   
   y = temp;
  
    printf("\nAfter Swapping we get the values: x = %d, y = %d", x, y);
    
    return 0;
}
